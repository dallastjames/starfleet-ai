{
  "name": "starfleet-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "supabase:link": "supabase link --project-ref $SUPABASE_PROJECT_REF -p $SUPABASE_PROJECT_PASSWORD",
    "supabase:reset": "supabase db reset && pnpm supabase:types:local",
    "supabase:types:linked": "supabase gen types typescript --linked > src/utils/database.ts &&  pnpm pretty-types",
    "supabase:types:local": "supabase gen types typescript --local > src/utils/database.ts && pnpm pretty-types",
    "pretty-types": "prettier --write src/utils/database.ts",
    "supabase:functions:deploy": "supabase functions deploy --import-map ./supabase/functions/import-map.json"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@solidjs/router": "^0.8.3",
    "@supabase/supabase-js": "^2.33.1",
    "clsx": "^2.0.0",
    "langchain": "^0.0.132",
    "solid-icons": "^1.0.11",
    "solid-js": "^1.7.8",
    "solid-toast": "^0.5.0",
    "tailwind-merge": "^1.14.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.6.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "supabase": "^1.88.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-solid": "^2.7.0"
  }
}
